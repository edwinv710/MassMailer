<%= simple_form_for(@server_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.collection_select(:server_ids, Server.all, :id, :name, {:include_blank => 'None'},  {multiple:true}) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
